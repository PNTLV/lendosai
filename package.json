{
  "name": "thenewone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "version:save": "git add . && git commit -m",
    "version:history": "git log --oneline --graph --decorate -20",
    "version:tree": "git log --oneline --graph --all --decorate",
    "version:list": "git log --oneline -20",
    "version:status": "git status",
    "version:backup": "mkdir -p VERSION_BACKUPS && cp -r app components lib VERSION_BACKUPS/backup-$(date +%Y%m%d-%H%M%S)/",
    "version:diff": "git diff HEAD~1",
    "version:show": "git show --stat",
    "version:reset-soft": "git reset --soft HEAD~1",
    "version:reset-hard": "git reset --hard HEAD~1"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.3",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
